{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "2-another-outbound-example",
  "properties": {
    "eventTimestamp": {
      "type": "string",
      "description": "Date-time value converted to UTC timezone, and represented in ISO format",
      "examples": ["2023-03-21T02:53:27.872Z"]
    },
    "eventId": {
      "type": "string",
      "description": "unique id generated by the system to identify the event",
      "examples": ["123e4567-e89b-12d3-a456-426614174000"]
    },
    "eventName": {
      "type": "string",
      "description": "Name of the event, used to identify the queue",
      "examples": ["UserCreated"]
    },
    "eventType": {
      "type": "string",
      "description": "Type of the event, used to categorize the message",
      "examples": ["Notification"]
    },
    "traceParent": {
      "type": "string",
      "examples": ["traceParent-001"]
    },
    "traceState": {
      "type": "string",
      "examples": ["traceState-success"]
    },
    "eventPayload": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string",
          "pattern": "^2\\d{7}$",
          "examples": ["21234567"]
        },
        "personalInformation": {
          "type": "object",
          "properties": {
            "firstName": {
              "type": "string",
              "maxLength": 256,
              "examples": ["John"]
            },
            "lastName": {
              "type": "string",
              "maxLength": 256,
              "examples": ["Doe"]
            },
            "email": {
              "type": "string",
              "nullable": true,
              "pattern": "^[\\w.-]+@[\\w.-]+\\.[a-zA-Z]{2,}$",
              "maxLength": 256,
              "examples": ["john.doe@example.com"]
            },
            "phoneNumber": {
              "type": "string",
              "nullable": true,
              "maxLength": 15,
              "pattern": "^\\+?[1-9]\\d{1,14}$",
              "examples": ["+12345678901"]
            }
          },
          "required": ["firstName", "lastName"],
          "additionalProperties": false
        }
      },
      "required": ["userId", "personalInformation"],
      "additionalProperties": false
    }
  },
  "required": ["eventTimestamp", "eventId", "eventName", "eventType", "eventPayload"],
  "additionalProperties": false
}