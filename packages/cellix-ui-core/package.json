{
  "name": "@cellix/ui-core",
  "version": "1.0.0",
  "description": "Core UI components library for Cellix applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "prebuild": "biome lint",
    "build": "tsc --build",
    "clean": "rimraf dist",
    "lint": "biome lint",
    "format": "biome format --write",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "antd": "^5.27.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "antd": ">=5.0.0"
  }
}