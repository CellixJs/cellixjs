{
  "extends": ["@cellix/typescript-config/base.json", "@docusaurus/tsconfig"],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "exactOptionalPropertyTypes": false,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", ".docusaurus", "build"]
}
